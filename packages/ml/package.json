{
  "name": "@small-ltsc/ml",
"version": "0.2.14",
  "description": "ML features for Small LTSC - Pattern importance scoring and quality prediction",
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build:esm && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "compression",
    "llm",
    "tokens",
    "transformer",
    "ml",
    "embeddings"
  ],
  "author": "Triage Sec <nicks@triage-sec.com>",
  "contributors": [
    "Nikhil Srivastava",
    "Omansh Bainsla",
    "Sahil Chatiwala"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triage-sec/small.git"
  },
  "bugs": {
    "url": "https://github.com/triage-sec/small/issues"
  },
  "homepage": "https://github.com/triage-sec/small#readme",
  "peerDependencies": {
"@small-ltsc/sdk": "^0.2.14"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
