{
  "name": "@small-ltsc/sdk",
"version": "0.2.14",
  "description": "TypeScript SDK for Small LTSC - Lossless Token Sequence Compression",
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./wasm": {
      "import": "./dist/esm/wasm/index.js",
      "types": "./dist/types/wasm/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build:esm && npm run build:types && npm run copy:wasm && npm run copy:json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "copy:wasm": "mkdir -p dist/esm/wasm/pkg && cp src/wasm/pkg/*.js src/wasm/pkg/*.wasm src/wasm/pkg/*.d.ts dist/esm/wasm/pkg/ 2>/dev/null || echo 'WASM copy - some files may be missing'",
    "copy:json": "mkdir -p dist/esm/dictionaries && cp src/dictionaries/*.json dist/esm/dictionaries/",
    "test": "vitest",
    "test:browser": "playwright test",
    "serve:test": "node ./scripts/serve-test.mjs",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "compression",
    "llm",
    "tokens",
    "transformer",
    "wasm",
    "lossless"
  ],
  "author": "Triage Sec <nicks@triage-sec.com>",
  "contributors": [
    "Nikhil Srivastava",
    "Omansh Bainsla",
    "Sahil Chatiwala"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triage-sec/small.git"
  },
  "bugs": {
    "url": "https://github.com/triage-sec/small/issues"
  },
  "homepage": "https://github.com/triage-sec/small#readme",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
